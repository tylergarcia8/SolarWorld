<p id="notice"><%= notice %></p>

<div class="pagination"><ul class="pagination">
  <li class="prev previous_page disabled">
    <a href="#">← Previous</a>
  </li> 
  <li class="active">
    <a rel="start" href="page=1">1</a>
  </li>
  <li>
   <a rel="next" href="page=2">2</a>
 </li>
 <li>
 <a href="page=3">3</a>
</li>
<li class="next next_page ">
 <a rel="next" href="page=2">Next →</a>
</li>
</ul>
</div>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Address</th>
      <th>Interest</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.age %></td>
      <td><%= user.address %></td>
      <td><%= user.interest %></td>
      <td><button type="button" class="btn btn-primary"><%= link_to 'Show', user %></button></td>
      <td><button type="button" class="btn btn-primary"><%= link_to 'Edit', edit_user_path(user) %></button></td>
      <td><button type="button" class="btn btn-primary" autocomplete="off"><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>

<button type="button" class="btn btn-primary">
  <%= link_to 'New User', new_user_path %>
</button>